{
  "theme": {
    "mode": "dark",
    "light": "Catppuccin Latte - No Italics",
    "dark": "Catppuccin Macchiato - No Italics"
  },
  "features": {
    "inline_completion_provider": "none"
  },
  "buffer_font_family": "Hack Nerd Font",
  "buffer_font_size": 16,
  "ui_font_family": "Inter",
  "ui_font_size": 16,
  "vim_mode": true,
  "cursor_blink": false,
  "auto_signature_help": true,
  "show_inline_completions": false,
  "calls": {
    "mute_on_join": true,
    "share_on_join": true
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false
  },
  "scrollbar": {
    "show": "always",
    "cursors": true,
    "git_diff": true,
    "search_results": true,
    "selected_symbol": true,
    "diagnostics": true
  },
  "gutter": {
    "line_numbers": true,
    "code_actions": true,
    "runnables": true,
    "folds": true
  },
  "indent_guides": {
    "enabled": true,
    "line_width": 1,
    "active_line_width": 1,
    "coloring": "fixed",
    "background_coloring": "disabled"
  },
  "vertical_scroll_margin": 999,
  "relative_line_numbers": true,
  "inlay_hints": {
    "enabled": false
  },
  "project_panel": {
    "button": true,
    "default_width": 240,
    "dock": "left",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 8,
    "auto_reveal_entries": true,
    "auto_fold_dirs": false
  },
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "chat_panel": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "assistant": {
    "version": "2",
    "enabled": false,
    "button": false
  },
  "autosave": "off",
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false
  },
  "tabs": {
    "git_status": true,
    "close_position": "right",
    "file_icons": true
  },
  "preview_tabs": {
    "enabled": false
  },
  "remove_trailing_whitespace_on_save": true,
  "ensure_final_newline_on_save": true,
  "format_on_save": "on",
  "preferred_line_length": 80,
  "tab_size": 2,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_update": false,
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**.g.dart",
    "**.freezed.dart"
  ],
  "code_actions_on_format": {
    "source.organizeImports": true,
    "source.fixAll": true
  },
  "auto_install_extensions": {
    "html": true,
    "log": true,
    "dart": true,
    "templ": true
  },
  "languages": {
    "Dart": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll": true
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "vim": {
    "use_system_clipboard": "always",
    "use_multiline_find": true,
    "use_smartcase_find": true
  }
}
