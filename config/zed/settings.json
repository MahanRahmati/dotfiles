{
  "icon_theme": {
    "mode": "system",
    "light": "Colored Zed Icons Theme Light",
    "dark": "Colored Zed Icons Theme Dark"
  },
  "features": {
    "edit_prediction_provider": "none"
  },
  "buffer_font_family": "Hack Nerd Font",
  "buffer_font_size": 16,
  "vim_mode": true,
  "helix_mode": false,
  "hover_popover_enabled": true,
  "cursor_blink": false,
  "cursor_shape": "bar",
  "show_completions_on_input": true,
  "show_completion_documentation": true,
  "auto_signature_help": true,
  "inline_code_actions": true,
  "calls": {
    "mute_on_join": true,
    "share_on_join": false
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false,
    "agent_review": false,
    "code_actions": false
  },
  "use_system_window_tabs": false,
  "title_bar": {
    "show_branch_icon": true,
    "show_branch_name": true,
    "show_project_items": true,
    "show_onboarding_banner": true,
    "show_user_picture": false,
    "show_sign_in": false,
    "show_menus": true
  },
  "scrollbar": {
    "show": "always",
    "cursors": true,
    "git_diff": true,
    "search_results": true,
    "selected_text": true,
    "selected_symbol": true,
    "diagnostics": "all"
  },
  "minimap": {
    "show": "never"
  },
  "gutter": {
    "line_numbers": true,
    "runnables": true,
    "breakpoints": true,
    "folds": true
  },
  "indent_guides": {
    "enabled": true
  },
  "vertical_scroll_margin": 999,
  "relative_line_numbers": true,
  "search": {
    "button": false
  },
  "inlay_hints": {
    "enabled": false
  },
  "project_panel": {
    "button": true
  },
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "git_panel": {
    "button": true,
    "dock": "right",
    "default_width": 360
  },
  "notification_panel": {
    "button": false
  },
  "agent": {
    "enabled": true,
    "preferred_completion_mode": "normal",
    "button": true,
    "dock": "right",
    "default_width": 420,
    "default_height": 320,
    "default_model": {
      "provider": "Llama.cpp",
      "model": "Local"
    },
    "editor_model": {
      "provider": "openai",
      "model": "bartowski/Qwen2.5-Coder-3B-Instruct-abliterated-GGUF:Q6_K"
    },
    "default_profile": "minimal"
  },
  "autosave": "off",
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": true
  },
  "tabs": {
    "git_status": true,
    "file_icons": true,
    "show_close_button": "hover",
    "show_diagnostics": "errors"
  },
  "preview_tabs": {
    "enabled": false
  },
  "file_finder": {
    "file_icons": true,
    "modal_max_width": "small"
  },
  "remove_trailing_whitespace_on_save": true,
  "extend_comment_on_newline": true,
  "ensure_final_newline_on_save": true,
  "format_on_save": "on",
  "preferred_line_length": 80,
  "hard_tabs": false,
  "tab_size": 2,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_update": false,
  "diagnostics": {
    "button": true
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/.jj",
    "**/.repo",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**.g.dart",
    "**.freezed.dart"
  ],
  "git": {
    "inline_blame": {
      "enabled": true
    }
  },
  "terminal": {
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "code_actions_on_format": {
    "source.organizeImports": true,
    "source.fixAll": true
  },
  "file_types": {
    "JSONC": [
      "**/.zed/**/*.json",
      "**/zed/**/*.json",
      "**/Zed/**/*.json",
      "**/.vscode/**/*.json",
      "tsconfig*.json"
    ],
    "Shell Script": [".env.*"]
  },
  "auto_install_extensions": {
    "log": true,
    "dart": true,
    "colored-zed-icons-theme": true
  },
  "completions": {
    "words": "enabled",
    "lsp": true,
    "lsp_fetch_timeout_ms": 0
  },
  "languages": {
    "Dart": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll": true
      }
    }
  },
  "language_models": {
    "openai_compatible": {
      "Llama.cpp": {
        "api_url": "http://localhost:8012",
        "available_models": [
          {
            "name": "Local",
            "max_tokens": 16384,
            "capabilities": {
              "tools": true,
              "images": false,
              "parallel_tool_calls": false,
              "prompt_cache_key": false
            }
          }
        ]
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "vim": {
    "use_system_clipboard": "always",
    "use_smartcase_find": true
  },
  "debugger": {
    "button": true
  }
}
