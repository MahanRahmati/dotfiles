{
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte - No Italics",
    "dark": "Catppuccin Macchiato - No Italics"
  },
  "icon_theme": {
    "mode": "system",
    "light": "Colored Zed Icons Theme Light",
    "dark": "Colored Zed Icons Theme Dark"
  },
  "features": {
    "edit_prediction_provider": "none"
  },
  "buffer_font_family": "Hack Nerd Font",
  "buffer_font_size": 16,
  "ui_font_family": "Inter",
  "ui_font_size": 16,
  "unnecessary_code_fade": 0.3,
  "active_pane_modifiers": {
    "magnification": 1.0,
    "border_size": 1.0,
    "inactive_opacity": 1.0
  },
  "bottom_dock_layout": "contained",
  "centered_layout": {
    "left_padding": 0.2,
    "right_padding": 0.2
  },
  "vim_mode": true,
  "hover_popover_enabled": true,
  "cursor_blink": false,
  "cursor_shape": "bar",
  "show_completions_on_input": true,
  "show_completion_documentation": true,
  "auto_signature_help": true,
  "calls": {
    "mute_on_join": true,
    "share_on_join": false
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false,
    "agent_review": false
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_branch_name": true,
    "show_project_items": true,
    "show_onboarding_banner": true,
    "show_user_picture": true
  },
  "scrollbar": {
    "show": "always",
    "cursors": true,
    "git_diff": true,
    "search_results": true,
    "selected_text": true,
    "selected_symbol": true,
    "diagnostics": "all"
  },
  "minimap": {
    "show": "never",
    "thumb": "always",
    "thumb_border": "left_open"
  },
  "gutter": {
    "line_numbers": true,
    "code_actions": true,
    "runnables": true,
    "breakpoints": true,
    "folds": true
  },
  "indent_guides": {
    "enabled": true,
    "line_width": 1,
    "active_line_width": 1,
    "coloring": "fixed",
    "background_coloring": "disabled"
  },
  "vertical_scroll_margin": 999,
  "autoscroll_on_clicks": false,
  "relative_line_numbers": true,
  "search": {
    "button": false,
    "whole_word": false,
    "case_sensitive": false,
    "include_ignored": false,
    "regex": false
  },
  "inlay_hints": {
    "enabled": false
  },
  "project_panel": {
    "button": true,
    "default_width": 240,
    "dock": "left",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "auto_reveal_entries": true,
    "auto_fold_dirs": false
  },
  "outline_panel": {
    "button": false,
    "default_width": 240,
    "dock": "left",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "auto_reveal_entries": true,
    "auto_fold_dirs": false
  },
  "collaboration_panel": {
    "button": false,
    "dock": "left",
    "default_width": 240
  },
  "chat_panel": {
    "button": "when_in_call",
    "dock": "right",
    "default_width": 240
  },
  "git_panel": {
    "button": false,
    "dock": "left",
    "default_width": 360,
    "status_style": "icon",
    "fallback_branch_name": "main"
  },
  "message_editor": {
    "auto_replace_emoji_shortcode": true
  },
  "notification_panel": {
    "button": false,
    "dock": "right",
    "default_width": 360
  },
  "agent": {
    "version": "2",
    "enabled": true,
    "preferred_completion_mode": "normal",
    "button": true,
    "dock": "right",
    "default_width": 420,
    "default_height": 320,
    "default_model": {
      "provider": "openai",
      "model": "bartowski/Qwen2.5-Coder-3B-Instruct-abliterated-GGUF:Q6_K"
    },
    "editor_model": {
      "provider": "openai",
      "model": "bartowski/Qwen2.5-Coder-3B-Instruct-abliterated-GGUF:Q6_K"
    },
    "default_profile": "minimal"
  },
  "slash_commands": {
    "docs": {
      "enabled": true
    },
    "project": {
      "enabled": true
    }
  },
  "autosave": "off",
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": true
  },
  "tabs": {
    "git_status": true,
    "close_position": "right",
    "file_icons": true,
    "show_close_button": "hover",
    "show_diagnostics": "errors"
  },
  "preview_tabs": {
    "enabled": false,
    "enable_preview_from_file_finder": false,
    "enable_preview_from_code_navigation": false
  },
  "file_finder": {
    "file_icons": true,
    "modal_max_width": "small"
  },
  "remove_trailing_whitespace_on_save": true,
  "extend_comment_on_newline": true,
  "ensure_final_newline_on_save": true,
  "format_on_save": "on",
  "preferred_line_length": 80,
  "hard_tabs": false,
  "tab_size": 2,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "auto_update": false,
  "diagnostics": {
    "button": true,
    "include_warnings": true,
    "inline": {
      "enabled": true,
      "update_debounce_ms": 150,
      "padding": 4,
      "min_column": 0,
      "max_severity": null
    }
  },
  "file_scan_exclusions": [
    "**/.git",
    "**/.svn",
    "**/.hg",
    "**/.jj",
    "**/CVS",
    "**/.DS_Store",
    "**/Thumbs.db",
    "**/.classpath",
    "**/.settings",
    "**.g.dart",
    "**.freezed.dart"
  ],
  "git": {
    "git_gutter": "tracked_files",
    "inline_blame": {
      "enabled": true
    },
    "hunk_style": "staged_hollow"
  },
  "edit_predictions": {
    "disabled_globs": [
      "**/.env*",
      "**/*.pem",
      "**/*.key",
      "**/*.cert",
      "**/*.crt",
      "**/.dev.vars",
      "**/secrets.yml"
    ],
    "mode": "eager",
    "enabled_in_text_threads": true
  },
  "terminal": {
    "shell": "system",
    "dock": "bottom",
    "default_width": 640,
    "default_height": 420,
    "button": false,
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "code_actions_on_format": {
    "source.organizeImports": true,
    "source.fixAll": true
  },
  "file_types": {
    "JSONC": [
      "**/.zed/**/*.json",
      "**/zed/**/*.json",
      "**/Zed/**/*.json",
      "**/.vscode/**/*.json"
    ],
    "Shell Script": [
      ".env.*"
    ]
  },
  "auto_install_extensions": {
    "log": true,
    "dart": true,
    "colored-zed-icons-theme": true
  },
  "completions": {
    "words": "enabled",
    "lsp": true,
    "lsp_fetch_timeout_ms": 0
  },
  "languages": {
    "Dart": {
      "code_actions_on_format": {
        "source.organizeImports": true,
        "source.fixAll": true
      }
    }
  },
  "language_models": {
    "openai": {
      "version": "1",
      "api_url": "http://localhost:8012",
      "available_models": [
        {
          "name": "bartowski/Qwen2.5-Coder-3B-Instruct-abliterated-GGUF:Q6_K",
          "display_name": "Qwen 2.5 coder 3B - 32K Token",
          "max_tokens": 32768
        }
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "vim": {
    "use_system_clipboard": "always",
    "use_multiline_find": true,
    "use_smartcase_find": true
  }
}
